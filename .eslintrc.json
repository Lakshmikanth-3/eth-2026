{
    "extends": [
        "next/core-web-vitals"
    ],
    "rules": {
        "no-console": "warn",
        "@typescript-eslint/no-explicit-any": "error",
        "@typescript-eslint/no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_",
                "varsIgnorePattern": "^_"
            }
        ],
        "prefer-const": "error",
        "no-var": "error"
    }
}